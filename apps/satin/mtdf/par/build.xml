<project
    name="ibis.apps.satin.mtdf.par"
    default="build"
    basedir=".">

    <property environment="env"/>

    <property name="ibis"        location="${env.IBIS_HOME}"/>
    <property name="build"       value="build"/>

    <property name="satin-classes" value="Mtdf"/>

    <property name="satinc-flags" value="-keep"/>

    <property name="ibis-run-args" value="-Dsatin.closed"/>
    <import file="${ibis}/build-files/apps/build-satin-app.xml"/>

    <target name="run"
	    depends="build"
	    description="Run test">
	<antcall target="run2">
		<param name="runjob" value="Mtdf"/>
	</antcall>
    </target>
</project>


