<project
    name="ibis.apps.satin.mtdf-othello.so"
    default="build"
    basedir=".">

    <property environment="env"/>

    <property name="ibis"        location="${env.IBIS_ROOT}"/>
    <property name="build"       value="build"/>

    <property name="satinc-flags" value="-keep"/>
    <property name="satinsoc-flags" value="-keep"/>
    <property name="main-class" value="Mtdf"/>
    <property name="satin-main-class" value="${main-class}"/>
    <property name="satin-so-classes" value="TranspositionTable"/>

    <property name="ibis-run-args" value="-Dsatin.closed"/>
    <property name="test-par-java-args" value="-Xmx256M"/>
    <property name="test-par-run"
	      value="Mtdf 2"/>
    <property name="test-seq-java-args" value="-Xmx256M"/>
    <property name="test-seq-run"
	      value="Mtdf 2"/>

    <import file="${ibis}/build-files/apps/build-satin-app.xml"/>

</project>
