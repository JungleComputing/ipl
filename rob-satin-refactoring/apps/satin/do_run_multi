/usr/java/bin/java -native -Xmx400M -Dfirewall_range=7000-8000 -Dibis.pool.host_number=13 -Dibis.pool.total_hosts=$RUNSIZE -Dibis.name_server.key=$POOL -Dibis.name_server.host=$NAMESERVER -Dibis.name_server.port=$NAMESERVER_PORT -Dibis.property.file=$HOME/projects/ibis/properties -classpath $HOME/projects:. $1 $2 -satin-closed -satin-stats -satin-ibis &

/usr/java/bin/java -native -Xmx400M -Dfirewall_range=7000-8000 -Dibis.pool.host_number=14 -Dibis.pool.total_hosts=$RUNSIZE -Dibis.name_server.key=$POOL -Dibis.name_server.host=$NAMESERVER -Dibis.name_server.port=$NAMESERVER_PORT -Dibis.property.file=$HOME/projects/ibis/properties -classpath $HOME/projects:. $1 $2 -satin-closed -satin-stats -satin-ibis &

/usr/java/bin/java -native -Xmx400M -Dfirewall_range=7000-8000 -Dibis.pool.host_number=15 -Dibis.pool.total_hosts=$RUNSIZE -Dibis.name_server.key=$POOL -Dibis.name_server.host=$NAMESERVER -Dibis.name_server.port=$NAMESERVER_PORT -Dibis.property.file=$HOME/projects/ibis/properties -classpath $HOME/projects:. $1 $2 -satin-closed -satin-stats -satin-ibis &
