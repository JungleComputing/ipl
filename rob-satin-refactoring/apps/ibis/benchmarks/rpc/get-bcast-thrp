#! /usr/bin/perl

@seq    = ( "sequenced", "nonsequenced" );
@ibises = ( "tcp", "net.bytes.gen.tcp_blk", "panda", "panda/inline", "net.gm" );
@nodes  = ( "8", "16", "32" );

while ( <> ) {
    chomp;
    # printf "current $_\n";
    if ($_ =~ /(\S+) ser (\S+)/) {
	$seq = $1;
	$ibis = $2;
	# printf "ibis $ibis; ser $ser\n";
    }
    if ($_ =~ /Run on (\S+) hosts/) {
	$node = $1;
	$is_all = 0;
    }
    if ($_ =~ /Kick-force/) {
    	$is_all = 1;
    }
    if ($_ =~ /Throughput ([0-9.]+) MB\/s/) {
	$thrp = $1;

	if ( $seq eq "sequenced" && $ibis ne "panda" ) {
	    $node -= 1;
	}

	# printf "ibis $ibis; seq $seq; thrp $thrp node $node\n";

	$ix = $ibis . "/" . $seq . "@" . $node . "all=" . $is_all;

	$average{ $ix } += $thrp;
	$n { $ix } ++;
	if ($max_thrp { $ix } == 0 || $thrp < $max_thrp { $ix } ) {
	    $max_thrp { $ix } = $thrp;
	}
    }
}

foreach ( ( "0", "1" ) ) {
    $is_all = $_;
    if ( $is_all == "0" ) {
	printf("Broadcast except to self\n");
    } else {
	printf("Broadcast including to self\n");
    }
    printf("%-4s %-24s %-16s %8s %8s %8s\n",
	    "N", "Ibis", "order", "hosts", "max(MB/s)", "av.(MB/s)");
    foreach ( @nodes ) {
	$node = $_;
	foreach ( @ibises ) {
	    $ibis = $_;
	    foreach ( @seq ) {
		$s = $_;
		$ix = $ibis . "/" . $s . "@" . $node . "all=" . $is_all;
		if ( $n { $ix } > 0) {
		    printf("%-4d %-24s %-16s %8d %8.1f %8.1f\n",
			    $n { $ix },
			    "$ibis", "$s", 
			    $node,
			    $max_thrp { $ix },
			    $average{ $ix } / $n { $ix });
		}
	    }
	}
    }
}

# foreach ( keys %average ) {
